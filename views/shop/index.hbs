{{# each products}}
    <div class="row">
        {{# each this}}
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="{{this.imagePath}}" alt="..." class="img-responsive">
                    <div class="caption">
                        <h3>{{this.title}}</h3>
                        <p>{{this.description}}</p>
                        <div class="clearfix">
                            <div class="price">{{this.price}} рублей</div>
                            <a href="/add-to-cart/" class="btn btn-success pull-right" role="button">Купить блять</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
{{/each}}

